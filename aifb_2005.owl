    <pages rdf:datatype="http://www.w3.org/2001/XMLSchema#string">314-319</pages>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Computing First-Order Logic Programs by Fibring Artificial Neural Networks</title>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2005</year>
  </rdf:Description>
