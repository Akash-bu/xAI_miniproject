    <pages rdf:datatype="http://www.w3.org/2001/XMLSchema#string">157-161</pages>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Model of an interorganizational Business Process Support System for public institutions</title>
    <volume rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1</volume>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2004</year>
  </rdf:Description>
