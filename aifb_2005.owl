    <number rdf:datatype="http://www.w3.org/2001/XMLSchema#string">9</number>
    <pages rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1407-1420</pages>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An Evolutionary Approach to Dynamic Task Scheduling on FPGAs with Restricted Buffer</title>
    <volume rdf:datatype="http://www.w3.org/2001/XMLSchema#string">62</volume>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2002</year>
