    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Interorganizational Business Process Management with XML Nets</title>
    <volume rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2472</volume>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2003</year>
