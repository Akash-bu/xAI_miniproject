    <number rdf:datatype="http://www.w3.org/2001/XMLSchema#string">P2195</number>
    <pages rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1007-1115</pages>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Model of Business Process Support System for E-Government</title>
