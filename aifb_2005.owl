    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Design Process in MIKE</title>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1994</year>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id56instance">
