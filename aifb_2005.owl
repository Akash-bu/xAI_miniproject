    <pages rdf:datatype="http://www.w3.org/2001/XMLSchema#string">972-986</pages>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mining Minimal Non-Redundant Association Rules Using Frequent Closed Itemsets</title>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2000</year>
  </rdf:Description>
