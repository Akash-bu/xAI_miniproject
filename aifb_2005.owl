    <booktitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">eEurope - IT Skills: Challenging Europe's Economic Future</booktitle>
    <month rdf:datatype="http://www.w3.org/2001/XMLSchema#string">April</month>
    <note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Workshop Proceedings eChallenges e-2003 Conference</note>
    <pages rdf:datatype="http://www.w3.org/2001/XMLSchema#string">9-19</pages>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Filling IT-Skills Gap and Shortage</title>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2004</year>
  </rdf:Description>
