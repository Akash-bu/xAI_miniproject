    <volume rdf:datatype="http://www.w3.org/2001/XMLSchema#string">14</volume>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1996</year>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id1185instance">
    <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#InProceedings"/>
