    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Specification Language KARL and Its Declarative Semantics</title>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1994</year>
  </rdf:Description>
