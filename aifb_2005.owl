    <month rdf:datatype="http://www.w3.org/2001/XMLSchema#string">January</month>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Model of Expertise in KARL</title>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1994</year>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewExternerAutorOWL/id751instance">
