    <isAbout rdf:resource="http://www.aifb.uni-karlsruhe.de/Forschungsgebiete/viewForschungsgebietOWL/id70instance"/>
    <pages rdf:datatype="http://www.w3.org/2001/XMLSchema#string">285â€“300</pages>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">User-Driven Ontology Evolution Management</title>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2002</year>
