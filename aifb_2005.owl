    <month rdf:datatype="http://www.w3.org/2001/XMLSchema#string">May</month>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Describing and Integrating Competence Theories for Problem Solving Components and Machine Learning Algorithms</title>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1996</year>
  </rdf:Description>
