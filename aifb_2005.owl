    <month rdf:datatype="http://www.w3.org/2001/XMLSchema#string">August</month>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hunting for Assumptions as Developing Method for Problem-Solving Methods</title>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1997</year>
  </rdf:Description>
