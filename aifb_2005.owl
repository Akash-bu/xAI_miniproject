    <pages rdf:datatype="http://www.w3.org/2001/XMLSchema#string">130-137</pages>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Translating Wh-Questions into F-Logic Queries</title>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2003</year>
  </rdf:Description>
