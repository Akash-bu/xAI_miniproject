    <journal rdf:datatype="http://www.w3.org/2001/XMLSchema#string">International Journal on Intelligent Systems (IJIS)</journal>
    <number rdf:datatype="http://www.w3.org/2001/XMLSchema#string">8</number>
    <pages rdf:datatype="http://www.w3.org/2001/XMLSchema#string">715-748</pages>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Role of Assumptions in Knowledge Engineering</title>
    <volume rdf:datatype="http://www.w3.org/2001/XMLSchema#string">13</volume>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1998</year>
