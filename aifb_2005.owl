    <pages rdf:datatype="http://www.w3.org/2001/XMLSchema#string">181-215</pages>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Essence of Problem-Solving Methods: Making Assumptions for Gaining Efficiency</title>
    <volume rdf:datatype="http://www.w3.org/2001/XMLSchema#string">48</volume>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1998</year>
