    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Guided Tour through the Data Mining Jungle</title>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1997</year>
  </rdf:Description>
