    <isAbout rdf:resource="http://www.aifb.uni-karlsruhe.de/Forschungsgebiete/viewForschungsgebietOWL/id12instance"/>
    <pages rdf:datatype="http://www.w3.org/2001/XMLSchema#string">243-263</pages>
